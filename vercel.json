{
  "framework": null,
  "installCommand": "cd ui && npm install",
  "buildCommand": "cd ui && npm run build",
  "outputDirectory": "dist/control-ui",
  "redirects": [
    {
      "source": "/",
      "destination": "/landing.html",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/chat",
      "destination": "/index.html"
    },
    {
      "source": "/chat/:path*",
      "destination": "/index.html"
    }
  ]
}
