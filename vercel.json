{
  "framework": null,
  "installCommand": "pnpm install --filter openclaw-control-ui... --no-frozen-lockfile",
  "buildCommand": "pnpm -C ui build",
  "outputDirectory": "dist/control-ui",
  "rewrites": [
    {
      "source": "/",
      "destination": "/landing.html"
    },
    {
      "source": "/chat",
      "destination": "/index.html"
    },
    {
      "source": "/chat/:path*",
      "destination": "/index.html"
    }
  ]
}
